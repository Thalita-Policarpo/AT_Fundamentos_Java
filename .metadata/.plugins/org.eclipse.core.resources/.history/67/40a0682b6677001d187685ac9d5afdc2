package br.edu.infnet.appcurso.model.domain;

public class BancoDeDados extends Curso {
	private String tipo;
	private String tipo;
	private String tipo;
	
	


	@Override
	public String retornarMensagem()  {
		
		return "O curso "  + getNomeCurso() + ", com duracao de " + getDuracao() + " foi cadastrado!";
	}
}
